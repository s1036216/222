package com.hanbit.oop.domain;

public class Gradebean {
	private int kor, eng, math;
	private String name2, major;

	public void setKor(int kor) {
		this.kor = kor;
	}

	public void setEng(int eng) {
		this.eng = eng;
	}

	public void setMath(int math) {
		this.math = math;
	}

	public int getKor() {
		return this.kor;
	}

	public int getEng() {
		return this.eng;
	}

	public int getMath() {
		return this.math;
	}
	
	public void setName1(String name2) {
		this.name2 = name2;
	}
	public String getName1() {
		return name2;
	}
	public void setMajor(String major) {
		this.major = major;
	}
	public String getMajor() {
		return major;
	}
}
