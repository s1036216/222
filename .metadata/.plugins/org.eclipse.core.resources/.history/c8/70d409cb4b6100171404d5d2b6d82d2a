package com.hanbit.board.domain;

public class BoardBean {
	 //번호
	private
	private String  seq, writer,title,content,regCate; //글쓴이,제목,내용,날짜
	public String getSeq() {
		return seq;
	}
	public void setSeq(String arr) {
		this.seq = arr;
	}
	public String getWriter() {
		return writer;
	}
	public void setWriter(String writer) {
		this.writer = writer;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getRegCate() {
		return regCate;
	}
	public void setRegCate(String regCate) {
//		this.regCate = regCate;
	}
	public String toString(){
		
		return null;
	}
	
}
