package com.hanbit.oop.sevice;

import java.util.Scanner;

public class LeapService {
	public String execute3(int year) {
String result="";
		if (year % 4 == 0) {
			result = "윤년";
			
		} else
			result = "평년";
		if (year % 100 == 0) {
			result = "윤년";
		} else
			result = "평년";

		if (year % 400 == 0) {
			result = "평년";
			System.out.println(result);
		
		}
		return result;
	}

}
