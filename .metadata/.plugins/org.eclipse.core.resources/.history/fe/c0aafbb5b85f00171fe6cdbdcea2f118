package com.hanbit.oop.sevice;

public class TaxService {
	public double execute2(int salary){
		
			
			double taxRate=0;
			int tax=(int)(salary*taxRate);
			if (salary<=1200){
				taxRate=0.08;
			}else if (4600>=salary&&salary>=1200){
				taxRate=0.17;
			}else if (8800>=salary&&salary>=4600){
				taxRate=0.26;
			}else if (8800<=salary){
				taxRate=0.35;
			}
					
	return taxRate;			
}
}
